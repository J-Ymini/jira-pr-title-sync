{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "outDir": "dist",
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
  },
  "tsc-alias": {
    "resolveFullPaths": true,
    "verbose": false,
  },
}
